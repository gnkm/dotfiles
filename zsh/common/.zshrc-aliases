alias gpw='cat /dev/urandom | LC_CTYPE=C tr -dc "[:alnum:]" | head -c 8'
alias grf='cat /dev/urandom | LC_CTYPE=C tr -dc "[:alnum:]" | head -c 16 | xargs touch'
# opensslの使用可能なもののリスト表示
alias osl='openssl ciphers ALL:@STRENGTH -v'
# 復号化．-in, -out は自分で与える．
alias dec='openssl -d -aes256'
# 暗号化
alias enc='openssl -e -aes256'
alias s='source ~/.zshrc'
alias tm='tmux'
alias tmns='tmux new -s $1'
alias va='vagrant'
