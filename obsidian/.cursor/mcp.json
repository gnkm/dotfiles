{
    "mcpServers": {
        "pinecone": {
            "command": "npx",
            "args": [
                "-y",
                "@pinecone-database/mcp"
            ],
            "env": {
                "PINECONE_API_KEY": "${PINECONE_API_KEY}"
            }
        },
        "context7": {
            "command": "npx",
            "args": [
                "-y",
                "@upstash/context7-mcp"
            ]
        }
    }
}
